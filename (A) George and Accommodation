#include <bits/stdc++.h>
using namespace std;
 
#define str string
#define ll long long
 
void solve(int n){
    int a[n], b[n], count = 0;
    for(int i = 0; i < n; i++){
        cin >> a[i] >> b[i];   
    }
    for(int i = 0; i < n; i++){
        if(abs(a[i]-b[i]) >= 2){
            count++;
        }
    }
    cout << count << '\n';
}
 
int main(){
    int a;
    cin >> a;
    solve(a);
    return 0;
}
