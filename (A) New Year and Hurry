#include <bits/stdc++.h>
using namespace std;
 
#define str string
#define ll long long
 
void solve(int n, int m){
    int cnt = 0, arr[n], rem = 240 - m;
    for(int i  = 0; i < n; i++){
        arr[i] = (i+1)*5;
    }
    for(int i = 0; i < n; i++){
        if(rem >= arr[i]){
            rem -= arr[i];
            cnt++;
        }else{
            break;
        }
    }
    cout << cnt;
}
int main(){
    int a, b;
    cin >> a >> b;
    solve(a,b);
    return 0;
}
