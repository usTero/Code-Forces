#include <bits/stdc++.h>
using namespace std;
 
#define str string
 
void solve(int n){
    str a[n];
    int b = 0;
    for(int i = 0; i < n; i++){
        cin >> a[i];
    }
    for(int i = 0; i < n; i++){
        if(a[i] == "X++" || a[i] == "++X"){
            b++;
        }else if(a[i] == "X--" || a[i] == "--X"){
            b--;
        }
    }
    cout << b << endl;
} 
 
int main(){
    int a;
    cin >> a;
    solve(a);
    return 0;
}
