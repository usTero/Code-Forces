#include <bits/stdc++.h>
using namespace std;
 
#define str string
#define ll long long
 
void solve(ll x, ll y, ll z){
    long long tilesX = (x + z - 1) / z;
    long long tilesY = (y + z - 1) / z;
    cout << tilesX * tilesY << '\n';
}
int main(){
    ll a, b, c;
    cin >> a >> b >> c;
    solve(a, b, c);
    return 0;
}
