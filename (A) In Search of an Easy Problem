#include <bits/stdc++.h>
using namespace std;
 
#define str string
#define ll long long
 
void solve(int n){
    int a[n];
    str dif = "EASY";
    for(int i = 0; i < n; i++){
        cin >> a[i];   
    }
    for(int i = 0; i < n; i++){
        if(a[i] == 1){
            dif = "HARD";
        }
    }
    cout << dif << '\n';
}
 
int main(){
    int a;
    cin >> a;
    solve(a);
    return 0;
}
