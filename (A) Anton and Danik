#include <bits/stdc++.h>
using namespace std;
 
#define str string
#define ll long long
 
void solve(int n){
    char a[n];
    int A = 0, D = 0;
    for(int i = 0; i < n; i++){
        cin >> a[i];
    }
    for(int i = 0; i < n; i++){
        if(a[i] == 'A'){
            A++;
        }else{
            D++;
        }
    }
    if(A > D){
        cout << "Anton";
    }else if(D > A){
        cout << "Danik";
    }else{
        cout << "Friendship";
    }
 
}
 
int main(){
    int a;
    cin >> a;
    solve(a);
    return 0;
}
