#include <bits/stdc++.h>
using namespace std;
 
#define str string
#define ll long long
 
void solve(int a, int b){
    int arr[a], counter = 0;
    for(int i = 0; i < a; i++){
        cin >> arr[i];
    }
    for(int j = 0; j < a;j++){
        if(arr[j] <= b){
            counter++;
        }else{
            counter+=2;
        }
    }
    cout << counter << '\n';
}
 
int main(){
    int a, b;
    cin >> a >> b;
    solve(a,b);
    return 0;
}
