#include <bits/stdc++.h>
using namespace std;
 
#define str string
#define ll long long
 
void solve(int n, int m){
    int count = 1, price = n;
    while(true){
        if(price % 10 == m || price % 10 == 0){
            break;
        }else{
            price += n;
            count++;
        }
    }
    cout << count;
}
 
int main(){
    int a, b;
    cin >> a >> b;
    solve(a,b);
    return 0;
}
